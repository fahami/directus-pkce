{
	"name": "@fahami/directus-pkce",
	"description": "A PKCE implementation for Directus",
	"icon": "extension",
	"version": "1.1.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-endpoint"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "17.0.4",
		"@types/ms": "^2.1.0",
		"@types/node": "^25.0.3",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@directus/memory": "^3.0.13",
		"ioredis": "^5.9.0",
		"ms": "^2.1.3",
		"short-unique-id": "^5.3.2"
	},
	"main": "dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fahami/directus-pkce.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/fahami/directus-pkce/issues"
	},
	"homepage": "https://github.com/fahami/directus-pkce#readme"
}